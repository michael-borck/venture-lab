{
  "productName": "VentureLab",
  "version": "1.0.0",
  "identifier": "com.venturelab.app",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "VentureLab - Where AI meets entrepreneurship",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "category": "Education",
    "targets": "all",
    "shortDescription": "AI-powered entrepreneurship toolkit",
    "longDescription": "VentureLab is a comprehensive AI-powered toolkit for entrepreneurs, featuring business idea generation, market research, pitch coaching, and product requirement document creation. Supports multiple AI providers including Ollama, OpenAI, Anthropic, and Google Gemini.",
    "homepage": "https://github.com/michael-borck/venture-lab",
    "license": "MIT",
    "copyright": "Copyright Â© 2024 VentureLab. All rights reserved.",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png", 
      "icons/128x128@2x.png",
      "icons/256x256.png",
      "icons/512x512.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "shell": {
      "open": true
    },
    "fs": {
      "requireLiteralLeadingDot": false
    }
  }
}
